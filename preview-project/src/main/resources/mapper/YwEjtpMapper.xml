<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.previewproject.dao.YwEjtpDao">

    <select id="getEjtpData" resultType="com.example.previewproject.entity.YwEjtp">
        select * from yw_ejtp
        <where>
            1=1
            and yjid=#{yjid}
            and zdid=#{zdid}
        </where>
    </select>


</mapper>